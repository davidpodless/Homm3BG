#. type: Bullet: '   - '
#: tools/translation-ai-prompts/prompt.txt:17
msgid "Start with an empty quoted string: `\"\"`"
msgstr "Start with an empty quoted string: `\"\"`"

#. type: Bullet: '   - '
#: tools/translation-ai-prompts/prompt.txt:17
msgid "Have each line on a new line"
msgstr "Have each line on a new line"

#. type: Bullet: '   - '
#: tools/translation-ai-prompts/prompt.txt:17
msgid "Have each line surrounded by quotes"
msgstr "Have each line surrounded by quotes"

#. type: Plain text
#: tools/translation-ai-prompts/prompt.txt:17
#, no-wrap
msgid ""
"   - End each quoted line with \\n\n"
"   Example:\n"
"   msgid \"\"\n"
